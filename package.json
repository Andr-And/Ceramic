   "license": "MIT",
   "keywords": [
      "ceramic",
      "anchor",
      "eth",
      "3box"
   ],
   "scripts": {
      "test": "./node_modules/.bin/mocha -r ts-node/register src/**/*.test.ts ",
      "coverage": "nyc npm run test",
      "build": "./node_modules/.bin/tsc -p tsconfig.json",
      "format": "prettier --write 'src/**/*{.ts,.tsx,.js}'",
      "prepublishOnly": "npm run build",
      "prebuild": "npm run clean",
      "postinstall": "node-config-ts",
      "lint": "./node_modules/.bin/tslint --fix -p tsconfig.json 'src/**/*{.ts,.tsx}' 'test/**/*{.ts,.tsx}'",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules//typeorm/cli.js",
      "clean": "rm -rf ./build; rm -rf coverage; rm -rf .nyc_output",
      "start": "node ./build/app.js"
   },
   "dependencies": {
      "@overnightjs/core": "^1.6.15",
      "@overnightjs/logger": "^1.1.10",
      "@types/node-cron": "^2.0.3",
