      "config.ts": "^1.0.0",
      "cors": "^2.8.5",
      "ejs": "^2.6.1",
      "ethers": "^4.0.46",
      "ethers-typescript-typings": "0.0.4",
      "express": "^4.17.1",
      "express-async-handler": "^1.1.4",
      "express-promise-router": "^3.0.3",
      "express-validator": "^6.1.0",
      "http-status-codes": "^1.3.2",
      "ipfs-http-client": "^42.0.0",
      "multihashes": "^0.4.19",
      "node-cron": "^2.0.3",
      "reflect-metadata": "^0.1.10",
      "simple-color-print": "^1.0.4",
      "typeorm": "^0.2.24",
      "uuid": "^7.0.3",
      "typestub-multibase": "^0.0.5",
      "typestub-multihashes": "^0.0.1",
      "cron-parser": "^2.13.0",
      "globby": "^11.0.0"
   },
   "devDependencies": {
      "@types/chai": "^4.2.11",
      "@types/cors": "^2.8.6",
      "@types/events": "^3.0.0",
      "@types/express": "^4.17.4",
      "@types/mocha": "^7.0.2",
      "@types/node": "^13.11.0",
      "@types/uuid": "^7.0.2",
      "chai": "^4.2.0",
      "ipfs": "^0.41.2",
      "jest": "^25.1.0",
      "mocha": "^7.1.1",
      "node-config-ts": "^3.0.4",
      "nyc": "^15.0.1",
      "prettier": "^2.0.2",
      "sqlite3": "^4.1.1",
      "tmp-promise": "^2.0.2",
      "ts-node": "^8.8.1",
      "tsconfig-paths": "^3.9.0",
      "tslint": "^6.1.1",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "^3.8.3"
   }
